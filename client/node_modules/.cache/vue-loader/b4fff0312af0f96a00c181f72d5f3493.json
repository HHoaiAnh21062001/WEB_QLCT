25px;\r\n    background-color: red;\r\n    padding: 0px !important;\r\n}\r\n\r\n.imgdelete {\r\n    border-radius: 25px;\r\n    margin: 3px 0px 10px 1px;\r\n    width: 20px;\r\n    height: 20px;\r\n}\r\n.range{\r\n    height: 100%;\r\n}\r\n.chi-tieu{\r\n    margin: 0px 10%;\r\n\r\n}\r\n</style>\r\n\r\n<template>\r\n    \r\n<div class=\"range\">\r\n    <div class=\"chung\">\r\n        <div class=\"them\">\r\n            <button class=\"btn_them\" v-on:click=\"themGiaoDich\">Thêm giao dịch</button>\r\n        </div>\r\n        <div class=\"filter\">\r\n            <input type=\"date\" class=\"filterinput\" id=\"date\" v-model=\"date\">\r\n            <input type=\"date\" class=\"filterinput\" id=\"date2\" v-model=\"date2\">\r\n            <button class=\"filter-item\" v-on:click=\"locNgay\">Lọc</button>\r\n        </div>\r\n    </div>\r\n    <div class=\"chi-tieu\">\r\n        <div class=\"describe\">\r\n            <div class=\"describe-item\">Ngày</div>\r\n            <div class=\"describe-item\">Số tiền</div>\r\n            <div class=\"describe-item\">danh mục</div>\r\n            <div class=\"describe-item\">Ghi chú</div>\r\n            <div class=\"describe-item\">chỉnh sửa</div>\r\n            <div class=\"describe-item\">Xóa</div>\r\n        </div>\r\n        <div class=\"item\" v-for=\"item in dsChiTieu\">\r\n            <div class=\"describe\">\r\n                <div class=\"describe-item\">{{ item.ngay }}</div>\r\n                <div class=\"describe-item\">{{ item.danhmuc }}</div>\r\n                <div class=\"describe-item\">{{ item.tien }}</div>\r\n                <div class=\"describe-item\">{{ item.ghichu }}</div>\r\n                <div class=\"describe-item\">{{ item.nguoichi }}</div>\r\n                <div class=\"describe-item\">\r\n                    <button class=\"nutXoa\" v-on:click=\"xoaGiaoDich(item.id)\">\r\n                        <img src=\"/img/delete.png\" alt=\"\" class=\"imgdelete\">\r\n                    </button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n    app.data = {\r\n    ...app.data,\r\n    showThemGiaoDich: false,\r\n    showXacNhanXoa: false,\r\n    idToDelete: null,\r\n    dsChiTieu: []\r\n}\r\n\r\n\r\napp.methods = {\r\n    ...app.methods,\r\n    async themGiaoDich() {\r\n        const response = await fetch(`http://localhost:2161/api/chitieu/${userId}`, {\r\n            method: 'POST',\r\n            headers: {\r\n                'Content-Type': 'application/json'\r\n            },\r\n            body: JSON.stringify(this.formChiTieu)\r\n        })\r\n        if (response.ok) {\r\n            this.loadChiTieu()\r\n            this.showThemGiaoDich = false\r\n            this.formChiTieu = {\r\n                ngay: '',\r\n                danhmuc: '',\r\n                tien: 0,\r\n                ghichu: '',\r\n                nguoichi: ''\r\n            }\r\n        }\r\n    },\r\n\r\n    async xoaGiaoDich(id) {\r\n        this.idToDelete = id\r\n        this.showXacNhanXoa = true\r\n    },\r\n\r\n    async xacNhanXoa() {\r\n        const response = await fetch(`/api/chitieu/${this.idToDelete}`, {\r\n            method: 'DELETE'\r\n        })\r\n        if (response.ok) {\r\n            this.loadChiTieu()\r\n            this.showXacNhanXoa = false\r\n            this.idToDelete = null\r\n        }\r\n    }\r\n}\r\n\r\nexport default {\r\n  name: 'chitieu',\r\n}\r\n</script>"]}]}