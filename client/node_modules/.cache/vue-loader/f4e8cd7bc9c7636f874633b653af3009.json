\n    display: inline-block;\r\n    float: left;\r\n    margin: 10px 0px;\r\n}\r\n\r\n.btn_them {\r\n    border: 1px solid black;\r\n}\r\n\r\n.filter-item {\r\n    margin-left: 5px;\r\n    font-size: 16px;\r\n    border: none;\r\n    border-radius: 25px;\r\n    background-color: rgb(255, 255, 255);\r\n}\r\n\r\n.filterinput {\r\n    border-radius: 25px;\r\n    border: none;\r\n}\r\n\r\n.describe {\r\n    display: grid;\r\n    grid-template-columns: 16.8% 20.9% 16.8% 16.8% 16.8% 11.8%;\r\n    column-gap: 0px;\r\n    background-color: rgb(105, 110, 255);\r\n}\r\n\r\n.describe-item {\r\n    display: grid;\r\n    text-align: center;\r\n    border: 1px solid black;\r\n    border-top: none;\r\n    border-bottom: none;\r\n    height: 50px;\r\n    align-items: center;\r\n}\r\n\r\n.nutXoa {\r\n    display: block;\r\n    margin: 10px auto;\r\n    width: 25px;\r\n    height: 25px;\r\n    border: 1px solid black !important;\r\n    border-radius: 25px;\r\n    background-color: red;\r\n    padding: 0px !important;\r\n}\r\n\r\n.imgdelete {\r\n    border-radius: 25px;\r\n    margin: 3px 0px 10px 1px;\r\n    width: 20px;\r\n    height: 20px;\r\n}\r\n\r\n.item {\r\n    background-color: rgb(198, 196, 196);\r\n}\r\n</style>\r\n\r\n<template>\r\n<div class=\"thunhap\">\r\n    <div class=\"them\">\r\n        <button class=\"btn_them\" v-on:click=\"themThuNhap\">Thêm thu nhập</button>\r\n    </div>\r\n    <div class=\"filter\">\r\n        <input type=\"date\" class=\"filterinput\" id=\"date\" v-model=\"date\">\r\n        <input type=\"date\" class=\"filterinput\" id=\"date2\" v-model=\"date2\">\r\n        <button class=\"filter-item\" v-on:click=\"locNgay\">Lọc</button>\r\n    </div>\r\n    <div class=\"thu-nhap\">\r\n        <div class=\"describe\">\r\n            <div class=\"describe-item\">Ngày</div>\r\n            <div class=\"describe-item\">Nguồn thu</div>\r\n            <div class=\"describe-item\">Số tiền</div>\r\n            <div class=\"describe-item\">Ghi chú</div>\r\n            <div class=\"describe-item\">Người nhận</div>\r\n            <div class=\"describe-item\">Xóa</div>\r\n        </div>\r\n        <div class=\"item\" v-for=\"item in dsThuNhap\">\r\n            <div class=\"describe\">\r\n                <div class=\"describe-item\">{{ item.ngay }}</div>\r\n                <div class=\"describe-item\">{{ item.nguonthu }}</div>\r\n                <div class=\"describe-item\">{{ item.tien }}</div>\r\n                <div class=\"describe-item\">{{ item.ghichu }}</div>\r\n                <div class=\"describe-item\">{{ item.nguoinhan }}</div>\r\n                <div class=\"describe-item\">\r\n                    <button class=\"nutXoa\" v-on:click=\"xoaThuNhap(item.id)\">\r\n                        <img src=\"/img/delete.png\" alt=\"\" class=\"imgdelete\">\r\n                    </button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'thunhap',\r\n    data() {\r\n        return {\r\n            dsThuNhap: [], // danh sách thu nhập\r\n            date: '', // ngày bắt đầu filter\r\n            date2: '', // ngày kết thúc filter\r\n            formThuNhap: {\r\n                ngay: '',\r\n                nguonthu: '',\r\n                tien: 0,\r\n                ghichu: '',\r\n                nguoinhan: ''\r\n            }\r\n        }\r\n    },\r\n    mounted() {\r\n        this.loadThuNhap()\r\n    },\r\n    methods: {\r\n        async loadThuNhap() {\r\n            try {\r\n                const response = await fetch('/api/thunhap')\r\n                if (response.ok) {\r\n                    const data = await response.json()\r\n                    this.dsThuNhap = data\r\n                }\r\n            } catch (error) {\r\n                console.error('Error:', error)\r\n            }\r\n        },\r\n\r\n        async themThuNhap() {\r\n            try {\r\n                const response = await fetch('/api/thunhap', {\r\n                    method: 'POST',\r\n                    headers: {\r\n                        'Content-Type': 'application/json'\r\n                    },\r\n                    body: JSON.stringify(this.formThuNhap)\r\n                })\r\n                if (response.ok) {\r\n                    await this.loadThuNhap()\r\n                    this.formThuNhap = {\r\n                        ngay: '',\r\n                        nguonthu: '',\r\n                        tien: 0,\r\n                        ghichu: '',\r\n                        nguoinhan: ''\r\n                    }\r\n                }\r\n            } catch (error) {\r\n                console.error('Error:', error)\r\n            }\r\n        },\r\n\r\n        async xoaThuNhap(id) {\r\n            try {\r\n                const response = await fetch(`/api/thunhap/${id}`, {\r\n                    method: 'DELETE'\r\n                })\r\n                if (response.ok) {\r\n                    await this.loadThuNhap()\r\n                }\r\n            } catch (error) {\r\n                console.error('Error:', error)\r\n            }\r\n        },\r\n\r\n        async locNgay() {\r\n            try {\r\n                const response = await fetch(`/api/thunhap/filter?from=${this.date}&to=${this.date2}`)\r\n                if (response.ok) {\r\n                    const data = await response.json()\r\n                    this.dsThuNhap = data\r\n                }\r\n            } catch (error) {\r\n                console.error('Error:', error)\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\nexport default {\r\n  name: 'thunhap',\r\n}\r\n</script>\r\n\r\n"]}]}